#	TIPO DE MENINGITE EM CASOS CONFIRMADOS:
CASE
   WHEN (CON_DIAGES) =1 THEN "MENINGOCOCEMIA"
   WHEN (CON_DIAGES) =2 THEN "MENINGITE MENINGOCÓCICA"
   WHEN (CON_DIAGES) =3 THEN "MENINGITE MENINGOCÓCICA COM MENINGOCOCEMIA"
   WHEN (CON_DIAGES) =4 THEN "MENINGITE TUBERCULOSA"
   WHEN (CON_DIAGES) =5 THEN "MENINGITE POR OUTRAS BACTÉRIAS"
   WHEN (CON_DIAGES) =6 THEN "MENINGITE NÃO ESPECIFICADA"
   WHEN (CON_DIAGES) =7 THEN "MENINGITE ASSÉPTICA"
   WHEN (CON_DIAGES) =8 THEN "MENINGITE DE OUTRA ETIOLOGIA"
   WHEN (CON_DIAGES) =9 THEN "MENINGITE POR HEMÓFILO"
   WHEN (CON_DIAGES) =10 THEN "MENINGITE POR PNEUMOCOCOS"
   ELSE "NÃO PREENCHIDO"
END

