#	AIDS/HIV:
CASE
   WHEN (ANT_AIDS)=1 THEN "SIM"
   WHEN (ANT_AIDS)=2 THEN "NÃO"
   WHEN (ANT_AIDS)=9 THEN "IGNORADO"
   ELSE "CAMPO VAZIO"
END

#	CONTATO COM CASO SUPEITO OU CONFIRMADO:
CASE
   WHEN (ANT_CONT_N) =1 THEN "DOMICÍLIO"
   WHEN (ANT_CONT_N) =2 THEN "VISINHANÇA"
   WHEN (ANT_CONT_N) =3 THEN "TRABALHO"
   WHEN (ANT_CONT_N) =4 THEN "CRECHE/ESCOLA"
   WHEN (ANT_CONT_N) =5 THEN "POSTO DE SAÚDE/HOSPITAL"
   WHEN (ANT_CONT_N) =6 THEN "OUTRO ESTADO OU MUNICÍPIO"
   WHEN (ANT_CONT_N) =7 THEN "SEM HISTÓRIA DE CONTATO"
   WHEN (ANT_CONT_N) =8 THEN "OUTRO PAÍS"
   WHEN (ANT_CONT_N) =9 THEN "IGNORADO"
   ELSE"NÃO PREENCHIDO"
END

#	CRITÉRIO DE CONFIRMAÇÃO:
CASE
   WHEN (CRITERIO) =1 THEN "CULTURA"
   WHEN (CRITERIO) =2 THEN "CIE"
   WHEN (CRITERIO) =3 THEN "AG. LATÉX"
   WHEN (CRITERIO) =4 THEN "CLÍNICO"
   WHEN (CRITERIO) =5 THEN "BACTERIOSCOPIA"
   WHEN (CRITERIO) =6 THEN "QUIMIOCITOLÓGICO DO LIQUOR"
   WHEN (CRITERIO) =7 THEN "CLÍNICO EPIDEMIOLÓGICO"
   WHEN (CRITERIO) =8 THEN "ISOLAMETO VIRAL"
   WHEN (CRITERIO) =9 THEN "PCR"
   WHEN (CRITERIO) =10 THEN "OUTROS"
   ELSE"NÃO PREENCHIDO"
END

#	EVOLUÇÃO
CASE
   WHEN (EVOLUCAO) =1 THEN "ALTA"
   WHEN (EVOLUCAO) =2 THEN "ÓBITO POR  MENINGITE"
   WHEN (EVOLUCAO) =3 THEN "ÓBITO POR OUTRAS CAUSAS"
   WHEN (EVOLUCAO) =4 THEN "IGNORADO"
   ELSE "NÃO PREENCHIDO"
END

