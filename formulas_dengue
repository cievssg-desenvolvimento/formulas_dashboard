# CLASSIFICAÇÃO:
CASE
   WHEN CLASSI_FIN =5 THEN "DESCARTADO"
   WHEN CLASSI_FIN =10 THEN "DENGUE"
   WHEN CLASSI_FIN =11 THEN "DENGUE COM SINAIS DE ALARME"
   WHEN CLASSI_FIN =12 THEN "DENGUE GRAVE"
   WHEN CLASSI_FIN =13 THEN "CHIKUNGUNYA"
   WHEN CLASSI_FIN =8 THEN "INOPORTUNO"
   ELSE "EM ANDAMENTO"
END  

# CRITÉRIO DE CONFIRMAÇÃO/DESCARTE:
CASE
   WHEN CRITERIO =1 THEN "LABORATORIAL"
   WHEN CRITERIO =2 THEN "CLÍNICO EPIDEMIOLÓGICO"
   WHEN CRITERIO =3 THEN "EM INVESTIGAÇÃO"
   ELSE"EM ANDAMENTO"
END


# MUNICÍPIO DE RESIDENCIA:
CASE
    WHEN (ID_MN_RESI)=330490 THEN "SÃO GONÇALO"
    WHEN (ID_MN_RESI)=330190 THEN "ITABORAI"
    WHEN (ID_MN_RESI)=330330 THEN "NITEROI"
    WHEN (ID_MN_RESI)=330250 THEN "MAGÉ"
    WHEN (ID_MN_RESI)=330455 THEN "RIO DE JANEIRO"
    WHEN (ID_MN_RESI)=330270 THEN "MARICÁ"
    WHEN (ID_MN_RESI)=330430 THEN "RIO BONITO"
    WHEN (ID_MN_RESI)=330390 THEN "PETRÓPOLIS"
    WHEN (ID_MN_RESI)=330350 THEN "NOVA IGUAÇÚ"
    WHEN (ID_MN_RESI)=330340 THEN "NOVA FRIBURGO"
    WHEN (ID_MN_RESI)=330510 THEN "SÃO JOÃO DE MERITI"
    WHEN (ID_MN_RESI)=330020 THEN "ARARUAMA"
    WHEN (ID_MN_RESI)=330170 THEN "DUQUE DE CAXIAS"
    ELSE "NÃO INFORMADO"
END


#	SOROTIPAGEM:
CASE
   WHEN SOROTIPO =1 THEN "DENGUE TIPO 1"
   WHEN SOROTIPO =2 THEN "DENGUE TIPO 2"
   WHEN SOROTIPO =3 THEN "DENGUE TIPO 3"
   WHEN SOROTIPO =4 THEN "DENGUE TIPO 4"
   ELSE "NÃO REALIZADO"
END

