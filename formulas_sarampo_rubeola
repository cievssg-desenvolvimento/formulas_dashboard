# AUTOCNE:
CASE
   WHEN (TPAUTOCTO) =1 THEN "SIM"
   WHEN (TPAUTOCTO) =2 THEN "NÃO"
   WHEN (TPAUTOCTO) =9 THEN "INDETERMINADO"
   ELSE "NÃO PREENCHIDO"
END

#	BLOQUEIO VACINAL:
CASE
   WHEN (CS_VACINAL) =1 THEN "SIM"
   WHEN (CS_VACINAL) =2 THEN "NÃO"
   WHEN (CS_VACINAL) =3 THEN "NÃO, TODOS VACINADOS"
   WHEN (CS_VACINAL) =4 THEN "NÃO SEM HISTÓRIA DE CONTATO"
   WHEN (CS_VACINAL) =9 THEN "IGNORADO"
   ELSE "NÃO PREENCHIDO"
END

#	CLASSIFICAÇÃO:
CASE
     WHEN CLASSI_FIN =1 THEN "SARAMPO"
     WHEN CLASSI_FIN =2 THEN "RUBÉOLA"
     WHEN CLASSI_FIN =3 THEN "DESCARTADO"
     ELSE "EM ANDAMENTO"
END  

#	CLASSIFICAÇÃO DO CASO DESCARTADO:
CASE
   WHEN CS_DESCART =1 THEN "DENGUE"
   WHEN CS_DESCART =2 THEN "ESCARLATINA"
   WHEN CS_DESCART =3 THEN "EXANTEMA SÚBITO (HERPES VÍRUS TIPO 6)"
   WHEN CS_DESCART =4 THEN "ERITEMA INFECCIOSO (PARVOVIRUS B19)"
   WHEN CS_DESCART =5 THEN "ENTEROVIROSE"
   WHEN CS_DESCART =6 THEN "EVENTO TEMPORAL RELACIONADO A VACINA"
   WHEN CS_DESCART =7 THEN "IGM TEMPORALMENTE ASSOCIADO A VACINA"
   WHEN CS_DESCART =8 THEN "SEM SOROCONVERSÃO DOS ANTICORPOS IGG"
   WHEN CS_DESCART =9 THEN "IGNORADO"
   ELSE"EM ANDAMENTO"
END

#	CONTATO COM CASO SUSPEITO OU CONFIRMADO:
CASE
   WHEN (CS_FONTE) =1 THEN "DOMICÍLIO"
   WHEN (CS_FONTE) =2 THEN "VISINHANÇA"
   WHEN (CS_FONTE) =3 THEN "TRABALHO"
   WHEN (CS_FONTE) =4 THEN "CRECHE/ESCOLA"
   WHEN (CS_FONTE) =5 THEN "POSTO DE SAÚDE/HOSPITAL"
   WHEN (CS_FONTE) =6 THEN "OUTRO ESTADO OU MUNICÍPIO"
   WHEN (CS_FONTE) =7 THEN "SEM HISTÓRIA DE CONTATO"
   WHEN (CS_FONTE) =8 THEN "OUTRO PAÍS"
   WHEN (CS_FONTE) =9 THEN "IGNORADO"
   ELSE"NÃO PREENCHIDO"
END

#	CRITÉRIO DE DESCARTE:
CASE
   WHEN (CRITERIO) =1 THEN "LABORATORIAL"
   WHEN (CRITERIO) =2 THEN "CLÍNICO EPIDEMIOLÓGICO"
   WHEN (CRITERIO) =3 THEN "CLÍNICO"
   WHEN (CRITERIO) =4 THEN "DATA DA ÚLTIMA DOSE DA VACINA"
   ELSE"EM ANDAMENTO"
END

#	ETIOLOGIA VIRAL:
CASE
   WHEN (ETIOL_OUTR) =1 THEN "VÍRUS SARAMPO SELVAGEM"
   WHEN (ETIOL_OUTR) =2 THEN "VÍRUS SARAMPO VACINAL"
   WHEN (ETIOL_OUTR) =3 THEN "VÍRUS RUBÉOLA SELVAGEM"
   WHEN (ETIOL_OUTR) =4 THEN "VÍRUS RUBÉOLA VACINAL"
   WHEN (ETIOL_OUTR) =5 THEN "DENGUE"
   WHEN (ETIOL_OUTR) =6 THEN "HERPES VÍRUS TIPO 6"
   WHEN (ETIOL_OUTR) =7 THEN "PARVOVÍRUS B19"
   WHEN (ETIOL_OUTR) =8 THEN "ENTEROVÍRUS"
   WHEN (ETIOL_OUTR) =9 THEN "OUTRAS"
   WHEN (ETIOL_OUTR) =1 THEN "NÃO DETECTADO"
   ELSE "NÃO PREENCHIDO"
END

#	EVOLUÇÃO:
CASE
   WHEN (EVOLUCAO) =1 THEN "CURA"
   WHEN (EVOLUCAO) =2 THEN "ÓBITO POR DOENÇAS EXANTEMÁTICAS"
   WHEN (EVOLUCAO) =3 THEN "ÓBITO POR OUTRAS CAUSAS"
   ELSE "EM ANDAMENTO"
END  

#	INTERVALO DE TEMPO DO BLOQUEIO:
CASE
   WHEN (INT_TEMPO) =1 THEN "EM ATE 72H"
   WHEN (INT_TEMPO) =2 THEN "APÓS 72H"
   WHEN (INT_TEMPO) =9 THEN "IGNORADO"
   ELSE"NÃO PREENCHIDO"
END

#	VACINADO CONTRA SARAMPO OU RUBÉOLA:
CASE
   WHEN (CS_VACINA) =1 THEN "SIM"
   WHEN (CS_VACINA) =2 THEN "NÃO"
   WHEN (CS_VACINA) =9 THEN "IGNORADO"
   ELSE "NÃO PREENCHIDO"
END
