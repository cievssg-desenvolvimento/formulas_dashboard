# FINANCEITRA/ECONÔMICA:
CASE
   WHEN (VIOL_FINAN) =1 THEN "SIM"
   WHEN (VIOL_FINAN) =2 THEN "NÃO"
   ELSE "IGNORADA"
END
# FÍSICA:
CASE
   WHEN (VIOL_FISIC) =1 THEN "SIM"
   WHEN (VIOL_FISIC ) =2 THEN "NÃO"
   ELSE "IGNORADA"
END

#	INTERVENÇÃO LEGAL:
CASE
   WHEN (VIOL_LEGAL) =1 THEN "SIM"
   WHEN (VIOL_LEGAL) =2 THEN "NÃO"
   ELSE "IGNORADA"
END

#	NEGLIGÊNCIA/ABANDONO:
CASE
   WHEN (VIOL_NEGLI) =1 THEN "SIM"
   WHEN (VIOL_NEGLI) =2 THEN "NÃO"
   ELSE "IGNORADA"
END

#	OUTROS:
CASE
   WHEN (VIOL_OUTR) =1 THEN "SIM"
   WHEN (VIOL_OUTR) =2 THEN "NÃO"
   ELSE "IGNORADA"
END

#	PSICOLÓGICA/MORAL:
CASE
   WHEN (VIOL_PSICO) =1 THEN "SIM"
   WHEN (VIOL_PSICO) =2 THEN "NÃO"
   ELSE "IGNORADA"
END

#	TORTURA:
CASE
   WHEN (VIOL_TORT) =1 THEN "SIM"
   WHEN (VIOL_TORT ) =2 THEN "NÃO"
   ELSE "IGNORADA"
END

#	TRABALHO INFANTIL:
CASE
   WHEN (VIOL_INFAN) =1 THEN "SIM"
   WHEN (VIOL_INFAN) =2 THEN "NÃO"
   ELSE "IGNORADA"
END

#	TRÁFICO DE SERES HUMANOS:
CASE
   WHEN (VIOL_TRAF) =1 THEN "SIM"
   WHEN (VIOL_TRAF) =2 THEN "NÃO"
   ELSE "IGNORADA"
END

#	RELAÇÃO COM PAI:
CASE
   WHEN (REL_PAI)=1 THEN "SIM"
   WHEN (REL_PAI)=2 THEN "NÃO"
   WHEN (REL_PAI)=9 THEN "IGNORADO"
   ELSE "CAMPO VAZIO"
END

#	SEXO AUTOR:
CASE
   WHEN (AUTOR_SEXO) =1 THEN "MASCULINO"
   WHEN  (AUTOR_SEXO)=2 THEN "FEMININO"
   WHEN (AUTOR_SEXO) =3 THEN "AMBOS OS SEXOS"
   WHEN  (AUTOR_SEXO)=9 THEN "IGNORADO"
   ELSE "CAMPO VAZIO"
END

#	SEXUAL:
CASE
   WHEN (VIOL_SEXU) =1 THEN "SIM"
   WHEN (VIOL_SEXU) =2 THEN "NÃO"
   ELSE "IGNORADA"
END

#	RAÇA:
CASE
   WHEN (CS_RACA) =1 THEN "BRANCA"
   WHEN (CS_RACA) =2 THEN "PRETA"
   WHEN (CS_RACA) =3 THEN "AMARELA"
   WHEN (CS_RACA) =4 THEN "PARDA"
   WHEN (CS_RACA) =5 THEN "INDIGENA"
   ELSE "IGNORADA"
END

#	SEXO:
CASE
   WHEN (CS_SEXO) ="F" THEN "FEMININO"
   WHEN  (CS_SEXO) ="M" THEN "MASCULINO"
   ELSE "IGNORADA"
END
