# ÁGUA OU LAMAS DE ENCHENTES:
CASE
   WHEN  ANT_CB_LAM=1 THEN "SIM"
   WHEN  ANT_CB_LAM=2 THEN "NÃO"
   WHEN  ANT_CB_LAM=4 THEN "IGNORADO"
   ELSE "EM BRANCO"
END

